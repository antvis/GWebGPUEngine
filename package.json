  "scripts": {
    "lint:fix": "prettier --write docs/api/**/*.md packages/**/*.{spec,story}.ts{,x} stories/**/**/*.tsx  *.md",
    "prebuild": "run-p tsc lint",
    "build": "yarn clean && lerna run build",
    "postbuild": "yarn build:declarations",
    "build:declarations": "lerna exec --stream --no-bail 'tsc --project ./tsconfig.build.json'",
    "fix": "run-p -c 'lint:ts-* --fix'",
    "lint:css": "stylelint 'packages/**/*.js{,x}'",
    "lint:ts-prod": "tslint --config tslint.prod.json 'packages/**/*.ts{,x}'",
    "lint:ts-test": "tslint --config tslint.test.json 'packages/**/*.{spec,story}.ts{,x}'",
    "lint:ts": "run-p -c lint:ts-*",
    "lint": "run-p -c lint:*",
    "commit": "git-cz",
    "version": "lerna version --force-publish --conventional-commits",
    "version:prerelease": "lerna version --conventional-prerelease",
    "prerelease": "yarn build",
    "release": "lerna publish from-package --registry https://registry.npmjs.com/ --force-publish",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o docs",
    "test": "cross-env BABEL_ENV=test jest",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "tsc": "tsc",
    "watch": "yarn clean && lerna exec --parallel -- cross-env BABEL_ENV=cjs babel --watch src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "clean": "lerna run clean"
  },
