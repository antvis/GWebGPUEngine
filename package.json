    "coveralls": "^3.0.7",
    "cross-env": "^7.0.2",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "dat.gui": "^0.7.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.0.0-beta6",
